@model IEnumerable<iFinance.Models.MonthlyExpens>
@{
    ViewBag.Title = "Monthly Expenses";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title.</h2>

<ul>
    @foreach (var p in Model)
    {
        <li>@Html.ActionLink(p.Name, "ViewCharge", "Home", new { ChargeName = p.Name}, null)</li>
    }
</ul>

